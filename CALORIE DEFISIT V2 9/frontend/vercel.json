{
    "version": 2,
    "name": "calorie-deficit-ai",
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/service-worker.js",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate",
          "Service-Worker-Allowed": "/"
        },
        "dest": "/service-worker.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  